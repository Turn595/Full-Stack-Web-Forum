ADA,CACA,UAAA,QAAA,EAAA;AACA,QAAA,KAAA,CAAA,IAAA,GAAA,QAAA,CAAA,IAAA;AACA,OAHA,EAIA,OAJA,CAIA,YAAA,CACA,CALA;AAMA;AARA,GAvBA;AAiCA,EAAA,OAjCA,qBAiCA;AAAA;;AACA,SAAA,SAAA,GAAA,KAAA,MAAA,CAAA,KAAA,CAAA,EAAA;;AACA,QAAA,KAAA,SAAA,IAAA,IAAA,EAAA;AACA,WAAA,OAAA,CAAA,KAAA,WAAA,EAAA,KAAA,EAAA;AAAA,QAAA,EAAA,EAAA,KAAA;AAAA,OAAA,EACA,IADA,CACA,UAAA,QAAA,EAAA;AACA,QAAA,MAAA,CAAA,OAAA,GAAA,QAAA,CAAA,IAAA,CAAA,CAAA,CAAA;AACA,OAHA,EAIA,OAJA,CAIA,YAAA;AACA,QAAA,MAAA,CAAA,OAAA;AACA,OANA;AAOA;AACA;AA5CA,CAAA","sourcesContent":["<template>\n\n  <div class=\"mt-5 border p-5 bg-light\">\n    <b-jumbotron :header=this.article.title :lead=this.article.subtitle>\n      <h5>Written By: {{this.article.username}}</h5>\n    </b-jumbotron>\n\n    <b-list-group class=\"m-4 flex-row flex-wrap\" horizontal>\n      <h3 class=\"mr-3\">Related Tags:</h3>\n      <b-list-group-item v-for=\"tag in tags\" :key=\"tag.id\" v-show=\"tags.length !== 0\">\n        {{tag.tag}}\n      </b-list-group-item>\n    </b-list-group>\n\n    <img class=\"border w-100\" style=\"max-height: 30em; object-fit:cover;\"  :src=this.article.bannerURL>\n\n    <div class=\"my-5\">\n      <p>{{this.article.body}}</p>\n    </div>\n\n    <commentSec :artID=\"articleID\"/>\n  </div>\n\n</template>\n\n<script>\nimport GlobalMixin from \"@/mixins/global-mixin\";\nimport commentSection from \"@/components/commentSection\";\n\nexport default {\n  name: \"articleContent\",\n  mixins:[GlobalMixin],\n\n  components:{\n    commentSec: commentSection\n  },\n  props:{\n    content:{\n      type:Object\n    }\n  },\n  data(){\n    return{\n      articleID: null,\n      article:{\n        type:Object\n      },\n      tags:{\n        type:Array\n      }\n    }\n  },\n  methods:{\n    getTags(){\n      this.callAPI(this.TAG_URL,'get', {'articleId':this.articleID})\n          .then(response => {\n            this.tags = response.data;\n          })\n          .finally(() => {\n      });\n    }\n  },\n    mounted() {\n    this.articleID = this.$route.query.id;\n    if (this.articleID != null){\n      this.callAPI(this.ARTICLE_URL, 'get', {id:this.articleID})\n          .then(response => {\n            this.article = response.data[0];\n          })\n          .finally(() => {\n          this.getTags();\n      });\n    }\n  }\n}\n</script>"],"sourceRoot":"src/components"}]}